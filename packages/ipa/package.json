{
  "name": "@nikitajs/ipa",
  "version": "1.0.0-alpha.9",
  "description": "Provides Nikita actions for various FreeIPA operations.",
  "keywords": [
    "nikita",
    "FreeIPA",
    "user",
    "identity",
    "pki",
    "tls",
    "ssl"
  ],
  "author": "David Worms <david@adaltas.com>",
  "bugs": {
    "url": "https://github.com/adaltas/node-nikita/issues"
  },
  "contributors": [
    {
      "name": "David Worms",
      "email": "david@adaltas.com"
    },
    {
      "name": "Sergei Kudinov",
      "email": "sergei@adaltas.com"
    }
  ],
  "dependencies": {
    "object-diff": "^0.0.4"
  },
  "devDependencies": {
    "@nikitajs/incus-runner": "^1.0.0-alpha.6",
    "coffeescript": "^2.7.0",
    "mixme": "^1.1.0",
    "mocha": "^10.7.0",
    "mocha-they": "^0.1.3",
    "should": "^13.2.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "exports": {
    "./register": "./lib/register.js",
    "./*": "./lib/*/index.js"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://nikita.js.org/",
  "license": "MIT",
  "mocha": {
    "inline-diffs": true,
    "loader": "../core/test/loaders/all.js",
    "recursive": true,
    "reporter": "spec",
    "require": [
      "@nikitajs/ipa/register",
      "should"
    ],
    "throw-deprecation": true,
    "timeout": 20000
  },
  "peerDependencies": {
    "@nikitajs/core": "^1.0.0-alpha.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-nikita",
    "directory": "packages/ipa"
  },
  "scripts": {
    "test": "npm run test:local && npm run test:env",
    "test:env": "env/run.sh",
    "test:local": "mocha 'test/**/*.coffee'"
  },
  "type": "module"
}
