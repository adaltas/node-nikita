{
  "name": "@nikitajs/ipa",
  "description": "Nikita FreeIPA actions.",
  "keywords": [
    "nikita",
    "FreeIPA",
    "user",
    "identity",
    "pki",
    "tls",
    "ssl"
  ],
  "version": "0.9.7",
  "author": "David Worms <david@adaltas.com>",
  "bugs": {
    "url": "https://github.com/adaltas/node-nikita/issues"
  },
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [
    {
      "name": "David Worms",
      "email": "david@adaltas.com"
    }
  ],
  "dependencies": {
    "object-diff": "^0.0.4"
  },
  "peerDependencies": {
    "@nikitajs/core": "^0.9.5"
  },
  "devDependencies": {
    "@nikitajs/lxd": "^0.9.7",
    "@nikitajs/tools": "^0.9.7",
    "coffeescript": "^2.5.1",
    "mixme": "^0.3.5",
    "mocha": "7.1.1",
    "should": "~13.2.3",
    "ssh2-they": "^2.1.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "/lib"
  ],
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register",
      "@nikitajs/ipa/src/register"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nikita.js.org/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-nikita",
    "directory": "packages/ipa"
  },
  "scripts": {
    "build": "coffee -b -o lib src && sed -i -e 's/src/lib/g' lib/register.js",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
