{
  "name": "@nikitajs/core",
  "description": "Automation and deployment solution of applications and infrastructures.",
  "keywords": [
    "build",
    "cli",
    "deploy",
    "fs",
    "install",
    "masson",
    "ssh",
    "system",
    "task"
  ],
  "version": "0.9.7-alpha.2",
  "author": "David Worms <david@adaltas.com>",
  "bugs": {
    "email": "open@adaltas.com",
    "url": "https://github.com/adaltas/node-nikita/issues"
  },
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    },
    "no_nested_string_interpolation": {
      "level": "ignore"
    }
  },
  "contributors": [
    {
      "name": "David Worms",
      "email": "david@adaltas.com"
    },
    {
      "name": "Pierre Sauvage",
      "email": "pierre@adaltas.com"
    },
    {
      "name": "Lucas Bakalian",
      "email": "lucas@adaltas.com"
    }
  ],
  "dependencies": {
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1",
    "colors": "~1.4.0",
    "cron-parser": "~2.13.0",
    "diff": "~4.0.2",
    "each": "^1.2.2",
    "ini": "~1.3.5",
    "js-yaml": "~3.13.1",
    "jsesc": "~2.5.2",
    "minimatch": "~3.0.4",
    "mixme": "^0.3.5",
    "moment": "~2.24.0",
    "mustache": "~4.0.0",
    "nunjucks": "~3.2.0",
    "pad": "~3.2.0",
    "regexp-quote": "~0.0.0",
    "semver": "^7.1.1",
    "ssh2-connect": "~2.0.5",
    "ssh2-exec": "~0.5.3",
    "ssh2-fs": "^0.3.8",
    "stack-trace": "~0.0.10",
    "tilde-expansion": "~0.0.0"
  },
  "optionalDependencies": {
    "season": "^6.0.2"
  },
  "devDependencies": {
    "coffeescript": "^2.5.0",
    "mocha": "7.0.0",
    "should": "~13.2.3",
    "ssh2-they": "^2.0.4"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nikita.js.org/",
  "license": "MIT",
  "main": "./lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-nikita",
    "directory": "packages/core"
  },
  "scripts": {
    "build": "coffee -b -o lib src",
    "pretest": "npm build",
    "test": "mocha test/{*,*/*,*/*/*}.coffee"
  }
}
